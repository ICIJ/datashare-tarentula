  "audio-type": {
    "query" : {
      "bool": {
        "must": [
          {
            "query_string": {
              "query": "name:*"
            }
          },
          {
            "terms" : {
              "contentType" : [
                "audio/mpeg",
                "audio/vnd.wave",
                "audio/mp4",
                "audio/vorbis",
                "audio/x-wav",
                "audio/x-ms-wma"
              ]
            }
          }
        ]
      }
    }
